{
  "extends": "./tsconfig.json",
  "exclude": [
    "src/**/*.test.ts",
    "src/**/__tests__/**/*",
    "tests/**/*",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false
  }
}